{
  "imports": {
    "hono": "jsr:@hono/hono@^4.9.9",
    "my-ui": "https://raw.githubusercontent.com/solidsnail/my-app/refs/heads/master/library/ui/index.tsx"
  },
  "tasks": {
    "start": "deno run --allow-net --allow-read --unstable-raw-imports src/index.tsx",
